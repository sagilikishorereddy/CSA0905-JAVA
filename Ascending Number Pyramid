public class NumberPattern1 {
    public static void main(String[] args) {
        int n = 5;  // You can change this to get a larger or smaller pyramid

        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }
}
